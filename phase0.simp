                            <item id="di726541151198913399" cid="RQ018" type="class" name="ImageToASCII" style-class="" visibility="public" creator="P58f25abd7a2f44afa57bebca6b18042e" creation-date="04/14/2021 06:02:09">
                              <tagged-values>

                                  <style class="default" />
                                  <source-codes>
                                    <source-code language="Java">
        moshi = new Moshi.Builder().build();
        ArrayList&lt;Class&gt; models = new ArrayList&lt;&gt;();
        models.add(User.class);
        try {
            db = Path.of("db");
            for (Class model : models) {
                dbs.put(model, Path.of("db\\" + model.getName().toLowerCase() + "s.json"));
            }
            updateUsersFromDB();
        } catch (Exception e) {
            Utils.printError("couldn't initialize database");
            System.exit(1);
        }
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="of5687e0836c6409eb35230041e59fdec543" cid="ELEM027" name="readFromFile" creation-date="01/01/0001 00:00:00" type="String" type-id="Uml.String" visibility="private" static="true">
                                  <style class="default" />
                                  <parameters>
                                    <parameter id="p6150dd09feb94bffbeec416e603254d7" name="jsonDB" type="Path" type-id="Path" />
                                  </parameters>
                                  <source-codes>
                                    <source-code language="Java">
        try {
            initDB(jsonDB);
            return Files.readString(jsonDB);
        } catch (Exception e) {
            Utils.printError("failed to read database");
            System.exit(1);
        }
        return null;
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="o3f3f89285b63430abe143d46b9bfdba4543" cid="ELEM028" name="writeToFile" creation-date="01/01/0001 00:00:00" type="" type-id="" visibility="private" static="true">
                                  <style class="default" />
                                  <parameters>
                                    <parameter id="pa2acf1280cb646af87cfeffa1ea09a7e" name="jsonDB" type="Path" type-id="Path" />
                                    <parameter id="p28035411bb5d4cdf959a82ce0b1b2fbd" name="jsonText" type="String" type-id="Uml.String" />
                                  </parameters>
                                  <source-codes>
                                    <source-code language="Java">
        try {
            initDB(jsonDB);
            Files.writeString(jsonDB, jsonText);
        } catch (Exception e) {
            Utils.printError("failed to write database");
            System.exit(1);
        }
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="o2580f981c3294958a6f407273f411cc0543" cid="ELEM029" name="initDB" creation-date="01/01/0001 00:00:00" type="" type-id="" visibility="private" static="true">
                                  <style class="default" />
                                  <parameters>
                                    <parameter id="pffa2e77174c14906874bcd47868f5971" name="jsonDB" type="Path" type-id="Path" />
                                  </parameters>
                                  <source-codes>
                                    <source-code language="Java">
        if (!Files.isDirectory(db)) {
            Files.createDirectory(db);
        }
        if (!Files.exists(jsonDB)) {
            Files.createFile(jsonDB);
            Files.writeString(jsonDB, "[]");
        }
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="o6775bf1dfebb496f8bf8025480b7252e543" cid="ELEM030" name="updateUsersFromDB" creation-date="01/01/0001 00:00:00" type="" type-id="" visibility="private" static="true">
                                  <style class="default" />
                                  <source-codes>
                                    <source-code language="Java">
        String users = "";
        try {
            users = readFromFile(dbs.get(User.class));
            Type type = Types.newParameterizedType(List.class, User.class);
            JsonAdapter&lt;List&lt;User&gt;&gt; usersAdapter = moshi.adapter(type);
            userList = usersAdapter.fromJson(users);
        } catch (Exception e) {
            System.out.println(users);
            Utils.printError("corrupted database");
            System.exit(1);
        }
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="o4eec4c7a9e594498a34b968c2253e7ae543" cid="ELEM031" name="updateUsersToDB" creation-date="01/01/0001 00:00:00" type="" type-id="" visibility="public" static="true">
                                  <style class="default" />
                                  <source-codes>
                                    <source-code language="Java">
        try {
            Type type = Types.newParameterizedType(List.class, User.class);
            JsonAdapter&lt;List&lt;User&gt;&gt; usersAdapter = moshi.adapter(type);
            writeToFile(dbs.get(User.class), usersAdapter.toJson(userList));
        } catch (Exception e) {
            Utils.printError("corrupted database");
            System.exit(1);
        }
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="o4ace5c42556d40c0a972e4e566b72740543" cid="ELEM032" name="getUserList" creation-date="01/01/0001 00:00:00" type="List&lt;User&gt;" type-id="List&lt;User&gt;" visibility="public" static="true">
                                  <style class="default" />
                                  <source-codes>
                                    <source-code language="Java">
        return userList;
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="o6e8e11efe9fb462c8c5ff26d0bd5ff06543" cid="ELEM033" name="addUser" creation-date="01/01/0001 00:00:00" type="" type-id="" visibility="public" static="true">
                                  <style class="default" />
                                  <parameters>
                                    <parameter id="pdbe5cd91476b4b678be0ec4ff83441b3" name="user" type="User" type-id="User" />
                                  </parameters>
                                  <source-codes>
                                    <source-code language="Java">
        userList.add(user);
        updateUsersToDB();
    </source-code>

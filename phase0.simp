                              <attributes>
                                <attribute id="a74ef687efdaf4ad1a5cc877b7d0590c9727" cid="ELEM134" name="db" creation-date="01/01/0001 00:00:00" type="Path" type-id="Path" visibility="private" static="true">
                                  <style class="default" />
                                </attribute>
                                <attribute id="aaac1ffe8c5ac41ec91b835460d848a25727" cid="ELEM135" name="dbs" creation-date="01/01/0001 00:00:00" type="HashMap&lt;String, Path&gt;" type-id="HashMap&lt;String, Path&gt;" visibility="private" static="true">
                                  <style class="default" />
                                </attribute>
                                <attribute id="a6249381fc47f4128aedd0e5f7a4db121727" cid="ELEM136" name="moshi" creation-date="01/01/0001 00:00:00" type="Moshi" type-id="Moshi" visibility="private" static="true">
                                  <style class="default" />
                                </attribute>
                                <attribute id="a3272f689492c4bed9743046e417a9038728" cid="ELEM137" name="userList" creation-date="01/01/0001 00:00:00" type="List&lt;User&gt;" type-id="List&lt;User&gt;" visibility="private" static="true">
                                  <style class="default" />
                                </attribute>
                              </attributes>
                              <operations>
                                <operation id="oa7a2daf17268466aa4dd7485cea552ab728" cid="ELEM138" name="init" creation-date="01/01/0001 00:00:00" type="" type-id="" visibility="public" static="true">
                                  <style class="default" />
                                  <source-codes>
                                    <source-code language="Java">
        moshi = new Moshi.Builder().build();
        dbs = new HashMap&lt;&gt;();
        ArrayList&lt;String&gt; models = new ArrayList&lt;&gt;();
        models.add("user");
        try {
            db = Path.of("db");
            for (String model : models) {
                dbs.put(model, Path.of("db\\" + model + "s.json"));
            }
            updateUsersFromDB();
        } catch (Exception e) {
            Utils.printError("couldn't initialize database");
            System.exit(1);
        }
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="oee958346f3fc4d1696d739c4d7a9893b728" cid="ELEM139" name="readFromFile" creation-date="01/01/0001 00:00:00" type="String" type-id="Uml.String" visibility="private" static="true">
                                  <style class="default" />
                                  <parameters>
                                    <parameter id="p00c1ba5a12864d3ca5997dd0c3f31f0f" name="jsonDB" type="Path" type-id="Path" />
                                  </parameters>
                                  <source-codes>
                                    <source-code language="Java">
        try {
            initDB(jsonDB);
            return Files.readString(jsonDB);
        } catch (Exception e) {
            Utils.printError("failed to read database");
            System.exit(1);
        }
        return null;
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="o1e0f24995e834835ad099932e902819c728" cid="ELEM140" name="writeToFile" creation-date="01/01/0001 00:00:00" type="" type-id="" visibility="private" static="true">
                                  <style class="default" />
                                  <parameters>
                                    <parameter id="p5a622e847f2840ddb05bb429db6345a4" name="jsonDB" type="Path" type-id="Path" />
                                    <parameter id="pa02c6e58934f440291a78ef7615450bc" name="jsonText" type="String" type-id="Uml.String" />
                                  </parameters>
                                  <source-codes>
                                    <source-code language="Java">
        try {
            initDB(jsonDB);
            Files.writeString(jsonDB, jsonText);
        } catch (Exception e) {
            Utils.printError("failed to write database");
            System.exit(1);
        }
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="oa3ffcfa10fa5443ab305eab0cf652f6f728" cid="ELEM141" name="initDB" creation-date="01/01/0001 00:00:00" type="" type-id="" visibility="private" static="true">
                                  <style class="default" />
                                  <parameters>
                                    <parameter id="p37b78d433d87406eade7b49ec780aa65" name="jsonDB" type="Path" type-id="Path" />
                                  </parameters>
                                  <source-codes>
                                    <source-code language="Java">
        if (!Files.isDirectory(db)) {
            Files.createDirectory(db);
        }
        if (!Files.exists(jsonDB)) {
            Files.createFile(jsonDB);
            Files.writeString(jsonDB, "[]");
        }
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="o2cb3a6b1528043e7bcf755d74f930d37728" cid="ELEM142" name="updateUsersFromDB" creation-date="01/01/0001 00:00:00" type="" type-id="" visibility="private" static="true">
                                  <style class="default" />
                                  <source-codes>
                                    <source-code language="Java">
        String users = "";
        try {
            users = readFromFile(dbs.get("user"));
            Type type = Types.newParameterizedType(List.class, User.class);
            JsonAdapter&lt;List&lt;User&gt;&gt; usersAdapter = moshi.adapter(type);
            userList = usersAdapter.fromJson(users);
        } catch (Exception e) {
            System.out.println(users);
            Utils.printError("corrupted database");
            System.exit(1);
        }
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="o2dafa822cacd44508c94d9698dc70d8f728" cid="ELEM143" name="updateUsersToDB" creation-date="01/01/0001 00:00:00" type="" type-id="" visibility="public" static="true">
                                  <style class="default" />
                                  <source-codes>
                                    <source-code language="Java">
        try {
            Type type = Types.newParameterizedType(List.class, User.class);
            JsonAdapter&lt;List&lt;User&gt;&gt; usersAdapter = moshi.adapter(type);
            writeToFile(dbs.get("user"), usersAdapter.toJson(userList));
        } catch (Exception e) {
            Utils.printError("corrupted database");
            System.exit(1);
        }
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="o9c50db24d7f84ae7bc5ec860008d593b728" cid="ELEM144" name="getUserList" creation-date="01/01/0001 00:00:00" type="List&lt;User&gt;" type-id="List&lt;User&gt;" visibility="public" static="true">
                                  <style class="default" />
                                  <source-codes>
                                    <source-code language="Java">
        return userList;
    </source-code>
                                  </source-codes>
                                </operation>
                                <operation id="o01467c2578be4293a9efd45ad35010d0728" cid="ELEM145" name="addUser" creation-date="01/01/0001 00:00:00" type="" type-id="" visibility="public" static="true">
                                  <style class="default" />
                                  <parameters>
                                    <parameter id="pfde669b5b79248bd95e3f2d7fe6bb559" name="user" type="User" type-id="User" />
                                  </parameters>
                                  <source-codes>
                                    <source-code language="Java">
        userList.add(user);
        updateUsersToDB();
    </source-code>
                                  </source-codes>
                                </operation>
                              </operations>
